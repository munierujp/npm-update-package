{
  "name": "npm-update-package",
  "version": "1.2.7",
  "description": "CLI tool for creating pull requests to update npm packages",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "eslint '**/*.{js,ts}'",
    "prebuild": "npm run clean",
    "prepare": "husky install",
    "prepublishOnly": "npm-run-all lint test build",
    "test": "jest"
  },
  "bin": "dist/src/bin/npm-update-package.js",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@octokit/rest": "18.12.0",
    "commander": "9.3.0",
    "execa": "5.1.1",
    "fp-ts": "2.12.1",
    "http-status-codes": "2.2.0",
    "io-ts": "2.2.16",
    "lodash": "4.14.2",
    "log4js": "6.5.2",
    "mustache": "4.1.0",
    "node-fetch": "2.6.7",
    "npm-check-updates": "13.0.4",
    "parse-github-url": "1.0.2",
    "semver": "7.3.7",
    "sleep-promise": "9.1.0",
    "type-guards": "0.15.0"
  },
  "devDependencies": {
    "@jest/types": "27.5.1",
    "@tsconfig/node14": "1.0.1",
    "@types/jest": "27.5.1",
    "@types/lodash": "4.14.182",
    "@types/mustache": "4.1.3",
    "@types/node": "14.18.18",
    "@types/node-fetch": "2.6.1",
    "@types/parse-github-url": "1.0.0",
    "@types/semver": "7.3.9",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "eslint": "8.16.0",
    "eslint-config-lodash": "1.1.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.4.6",
    "eslint-plugin-lodash": "7.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-tsdoc": "0.2.16",
    "eslint-plugin-unicorn": "42.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.12.0",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "lint-staged": "12.5.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.5",
    "ts-node": "10.8.0",
    "typescript": "4.7.2",
    "utility-types": "3.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npm-update-package/npm-update-package.git"
  },
  "bugs": {
    "url": "https://github.com/npm-update-package/npm-update-package/issues"
  },
  "homepage": "https://github.com/npm-update-package/npm-update-package",
