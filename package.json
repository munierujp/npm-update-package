{
  "name": "npm-update-package",
  "version": "0.24.2",
  "description": "CLI tool for creating pull requests to update npm packages",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "eslint '**/*.{js,ts}'",
    "prebuild": "npm run clean",
    "prepare": "husky install",
    "prepublish": "npm-run-all lint test build",
    "start": "ts-node src/bin.ts",
    "test": "jest --passWithNoTests"
  },
  "files": [
    "dist"
  ],
  "bin": "dist/bin.js",
  "dependencies": {
    "@octokit/rest": "18.12.0",
    "commander": "8.3.0",
    "execa": "5.1.1",
    "fp-ts": "2.11.8",
    "io-ts": "2.2.16",
    "log4js": "6.3.0",
    "mustache": "4.1.0",
    "npm-check-updates": "12.1.0",
    "parse-github-url": "1.0.2",
    "semver": "7.3.5",
    "type-guards": "0.15.0"
  },
  "devDependencies": {
    "@jest/types": "27.4.2",
    "@tsconfig/node12": "1.0.9",
    "@types/jest": "27.4.0",
    "@types/mustache": "4.1.2",
    "@types/node": "12.20.40",
    "@types/parse-github-url": "1.0.0",
    "@types/semver": "7.3.9",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "eslint": "8.7.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "25.7.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.2.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "utility-types": "3.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npm-update-package/npm-update-package.git"
  },
  "bugs": {
    "url": "https://github.com/npm-update-package/npm-update-package/issues"
  },
  "homepage": "https://github.com/npm-update-package/npm-update-package",
  "license": "MIT"
}
