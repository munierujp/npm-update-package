{
  "name": "npm-update-package",
  "version": "1.5.13",
  "description": "CLI tool for creating pull requests to update npm packages",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "eslint '**/*.{js,ts}'",
    "lint:fix": "eslint --fix '**/*.{js,ts}'",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm-run-all lint test build",
    "test": "jest"
  },
  "bin": "dist/src/bin/npm-update-package.js",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@octokit/rest": "19.0.7",
    "array-shuffle": "2.0.0",
    "commander": "9.5.0",
    "execa": "5.1.1",
    "fp-ts": "2.13.1",
    "http-status-codes": "2.2.0",
    "io-ts": "2.2.20",
    "log4js": "6.7.1",
    "mustache": "4.2.0",
    "node-fetch": "2.6.7",
    "npm-check-updates": "16.3.11",
    "parse-github-url": "1.0.2",
    "semver": "7.3.8",
    "type-guards": "0.15.0"
  },
  "devDependencies": {
    "@eslint-recommended/eslint-config-typescript": "12.0.0",
    "@npm-update-package/eslint-config-typescript": "1.0.0",
    "@tsconfig/node14": "1.0.3",
    "@types/mustache": "4.2.2",
    "@types/node": "14.18.34",
    "@types/node-fetch": "2.6.2",
    "@types/parse-github-url": "1.0.0",
    "@types/semver": "7.3.13",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "eslint": "8.34.0",
    "eslint-config-standard-with-typescript": "34.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-import-newlines": "1.3.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsdoc": "40.0.0",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-sonarjs": "0.18.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-unicorn": "45.0.2",
    "jest": "29.3.1",
    "npm-run-all": "4.1.5",
    "rimraf": "4.1.2",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "utility-types": "3.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npm-update-package/npm-update-package.git"
  },
  "bugs": {
    "url": "https://github.com/npm-update-package/npm-update-package/issues"
  },
  "homepage": "https://github.com/npm-update-package/npm-update-package",
  "license": "MIT",
  "keywords": [
    "dependencies",
    "dependency",
    "github enterprise",
    "github",
    "node",
    "npm",
    "outdated",
    "package",
    "update",
    "yarn"
  ]
