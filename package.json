{
  "name": "npm-update-package",
  "version": "4.0.7",
  "description": "CLI tool for creating pull requests to update npm packages",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "eslint '**/*.{js,mjs,cjs,ts,mts,cts}'",
    "postversion": "npm run syncVersion",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm-run-all lint test build",
    "syncVersion": "tsx scripts/syncVersion.ts",
    "test": "glob -c 'tsx --test' 'src/**/*.test.ts'"
  },
  "bin": "dist/bin/npm-update-package.js",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@octokit/rest": "20.1.1",
    "commander": "12.0.0",
    "execa": "9.1.0",
    "fp-ts": "2.16.5",
    "http-status-codes": "2.3.0",
    "io-ts": "2.2.21",
    "lodash": "4.17.21",
    "log4js": "6.9.1",
    "mustache": "4.2.0",
    "npm-check-updates": "16.3.11",
    "parse-github-url": "1.0.2",
    "semver": "7.5.4",
    "type-guards": "0.15.0"
  },
  "devDependencies": {
    "@munierujp/eslint-config-typescript": "46.0.1",
    "@tsconfig/node20": "20.1.4",
    "@types/lodash": "4.17.4",
    "@types/mustache": "4.2.5",
    "@types/node": "20.12.12",
    "@types/parse-github-url": "1.0.3",
    "@types/semver": "7.5.8",
    "eslint": "8.57.0",
    "eslint-config-lodash": "4.0.1",
    "glob": "10.3.15",
    "nock": "14.0.0-beta.6",
    "npm-run-all2": "6.2.0",
    "rimraf": "5.0.7",
    "test-each": "7.0.0",
    "tsx": "4.10.1",
    "typescript": "5.4.5",
    "utility-types": "3.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npm-update-package/npm-update-package.git"
  },
  "bugs": {
    "url": "https://github.com/npm-update-package/npm-update-package/issues"
  },
  "homepage": "https://github.com/npm-update-package/npm-update-package",
  "license": "MIT",
  "keywords": [
    "dependencies",
    "dependency",
    "github enterprise",
    "github",
    "node",
    "npm",
    "outdated",
    "package",
    "update",
    "yarn"
  ]
}
