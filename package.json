{
  "name": "npm-update-package",
  "version": "2.0.1",
  "description": "CLI tool for creating pull requests to update npm packages",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "eslint '**/*.{js,ts}'",
    "lint:fix": "eslint --fix '**/*.{js,ts}'",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm-run-all lint test build",
    "test": "jest"
  },
  "bin": "dist/src/bin/npm-update-package.js",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@octokit/rest": "19.0.8",
    "array-shuffle": "2.0.0",
    "commander": "10.0.1",
    "execa": "5.1.1",
    "fp-ts": "2.15.0",
    "http-status-codes": "2.2.0",
    "io-ts": "2.2.20",
    "log4js": "6.9.1",
    "mustache": "4.2.0",
    "node-fetch": "2.6.7",
    "npm-check-updates": "16.3.11",
    "parse-github-url": "1.0.2",
    "semver": "7.3.8",
    "type-guards": "0.15.0"
  },
  "devDependencies": {
    "@npm-update-package/eslint-config-typescript": "2.0.0",
    "@tsconfig/node16": "1.0.4",
    "@types/mustache": "4.2.2",
    "@types/node": "20.2.3",
    "@types/node-fetch": "2.6.3",
    "@types/parse-github-url": "1.0.0",
    "@types/semver": "7.3.13",
    "eslint": "8.39.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.5.0",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "utility-types": "3.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npm-update-package/npm-update-package.git"
  },
  "bugs": {
    "url": "https://github.com/npm-update-package/npm-update-package/issues"
  },
  "homepage": "https://github.com/npm-update-package/npm-update-package",
  "license": "MIT",
  "keywords": [
    "dependencies",
    "dependency",
    "github enterprise",
    "github",
    "node",
    "npm",
    "outdated",
    "package",
    "update",
    "yarn"
  ]
}
